model BoxGame {
  id            Int      @id @default(autoincrement())
  date          DateTime @default(now()) @map("Date")
  name          String   @db.VarChar(16)
  gameOfThatDay Int      @map("gameOfThatDay")
  awayTeamId    Int?     @map("visitorTeamID")
  homeTeamId    Int?     @map("homeTeamID")
  attendance    Int?     @map("attendance")
  capacity      Int?     @map("capacity")
  visitorWins   Int      @default(0) @map("visitorWins")
  visitorLosses Int      @default(0) @map("visitorLosses")
  homeWins      Int      @default(0) @map("homeWins")
  homeLosses    Int      @default(0) @map("homeLosses")
  visitorQ1pts  Int      @default(0) @map("visitorQ1points")
  visitorQ2pts  Int      @default(0) @map("visitorQ2points")
  visitorQ3pts  Int      @default(0) @map("visitorQ3points")
  visitorQ4pts  Int      @default(0) @map("visitorQ4points")
  visitorOTpts  Int      @default(0) @map("visitorOTpoints")
  homeQ1pts     Int      @default(0) @map("homeQ1points")
  homeQ2pts     Int      @default(0) @map("homeQ2points")
  homeQ3pts     Int      @default(0) @map("homeQ3points")
  homeQ4pts     Int      @default(0) @map("homeQ4points")
  homeOTpts     Int      @default(0) @map("homeOTpoints")
  min           Int      @default(0) @map("gameMIN")
  fgm           Int      @default(0) @map("gameFGM")
  fga           Int      @default(0) @map("gameFGA")
  ftm           Int      @default(0) @map("gameFTM")
  fta           Int      @default(0) @map("gameFTA")
  tpm           Int      @default(0) @map("game3GM")
  tpa           Int      @default(0) @map("game3GA")
  orb           Int      @default(0) @map("gameORB")
  drb           Int      @default(0) @map("gameDRB")
  ast           Int      @default(0) @map("gameAST")
  stl           Int      @default(0) @map("gameSTL")
  tov           Int      @default(0) @map("gameTOV")
  blk           Int      @default(0) @map("gameBLK")
  pf            Int      @default(0) @map("gamePF")

  @@map("ibl_box_scores_teams")
}
